{
  "name": "Kasaya recorder",
  "version": "1.0",
  "description": "generate kasaya script",
  "permissions": ["storage"],
  "browser_action": {
    "default_title": "Kasaya recorder",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": {
    "js": ["listeners/index.js" ],
    "matches": ["<all_urls>"]
  },
  "manifest_version": 2
}
